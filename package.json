{
	"name": "tanstack-start-starter",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && tsc --noEmit",
		"serve": "vite preview",
		"test": "vitest run",
		"typecheck": "tsc",
		"lint": "biome lint .",
		"lint:fix": "biome lint --write .",
		"format": "biome format --write .",
		"format:check": "biome format .",
		"check": "biome check . && pnpm typecheck",
		"fix": "biome check --write .",
		"prepare": "husky"
	},
	"lint-staged": {
		"*.{ts,tsx,js,jsx,json,jsonc}": [
			"biome check --write --no-errors-on-unmatched"
		]
	},
	"dependencies": {
		"@gsap/react": "^2.1.2",
		"@tailwindcss/vite": "^4.1.18",
		"@tanstack/react-devtools": "^0.8.6",
		"@tanstack/react-router": "^1.158.1",
		"@tanstack/react-router-devtools": "^1.158.1",
		"@tanstack/react-start": "^1.158.1",
		"@types/three": "^0.182.0",
		"clsx": "^2.1.1",
		"gsap": "^3.14.2",
		"html-react-parser": "^5.2.16",
		"lenis": "^1.3.17",
		"lucide-react": "^0.553.0",
		"motion": "^12.31.0",
		"next-themes": "^0.4.6",
		"react": "^19.2.4",
		"react-aria-components": "^1.15.0",
		"react-dom": "^19.2.4",
		"react-hook-form": "^7.71.1",
		"recharts": "^3.7.0",
		"tailwind-merge": "^3.4.0",
		"tailwind-variants": "^3.2.2",
		"tailwindcss": "^4.1.18",
		"three": "^0.182.0",
		"vite": "^7.3.1",
		"zustand": "^5.0.11"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.14",
		"@tanstack/devtools-vite": "^0.3.12",
		"@tanstack/router-plugin": "^1.158.1",
		"@testing-library/dom": "^10.4.1",
		"@testing-library/react": "^16.3.2",
		"@types/node": "^24.10.10",
		"@types/react": "^19.2.11",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react-swc": "^4.2.3",
		"husky": "^9.1.7",
		"jsdom": "^27.4.0",
		"lint-staged": "^16.2.7",
		"typescript": "^5.9.3",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^4.0.18"
	}
}
